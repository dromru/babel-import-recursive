{
  "name": "babel-plugin-import-recursive",
  "version": "1.1.3",
  "description": "A babel plugin that unrolls wildcard imports.",
  "main": "lib/index.js",
  "files": [
    "lib/index.js"
  ],
  "keywords": [
    "babel",
    "import",
    "recursive",
    "wildcard"
  ],
  "scripts": {
    "test": "jest",
    "build": "rimraf lib && babel -d ./lib ./src --ignore ./src/test",
    "prepare": "rimraf lib && babel -d ./lib ./src --ignore ./src/test",
    "lint": "eslint",
    "prettify": "prettier"
  },
  "dependencies": {
    "@babel/template": "^7.10.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.11.5",
    "eslint": "7.11.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "jest": "^26.5.3",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": "https://github.com/dromru/babel-plugin-import-recursive.git",
  "author": "dromru",
  "license": "MIT"
}
