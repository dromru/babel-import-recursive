{
  "name": "babel-plugin-import-recursive",
  "version": "1.1.3",
  "description": "A babel plugin that unrolls wildcard imports.",
  "main": "lib/index.js",
  "files": [
    "lib/index.js"
  ],
  "keywords": [
    "babel",
    "import",
    "recursive",
    "wildcard"
  ],
  "scripts": {
    "test": "jest",
    "build": "rimraf lib && babel -d ./lib ./src --ignore ./src/test",
    "prepare": "rimraf lib && babel -d ./lib ./src --ignore ./src/test",
    "lint": "eslint",
    "prettify": "prettier"
  },
  "dependencies": {
    "@babel/template": "^7.10.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/register": "^7.12.10",
    "eslint": "7.18.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": "https://github.com/dromru/babel-plugin-import-recursive.git",
  "author": "dromru",
  "license": "MIT"
}
